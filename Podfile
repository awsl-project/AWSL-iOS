# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'
inhibit_all_warnings!

target 'AWSL' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AWSL
  pod 'Alamofire'
  pod 'SnapKit'
  pod 'Kingfisher'
  pod 'R.swift'
end

target 'AWSLWidgetExtension' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'Kingfisher'
end
